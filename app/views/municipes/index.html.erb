<p id="notice"><%= notice %></p>

<h1>Municipes</h1>

<div>
  <%= search_form_for @search, url: municipes_path do |f| %>
    <%= f.search_field :name_or_cpf_or_cns_or_phone_or_addresses_street_or_addresses_city_or_addresses_ibge_code_or_addresses_neighborhood_or_addresses_complement_cont %>
    <%= submit_tag 'Buscar' %>
  <% end %>
</div>
<br />
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Cpf</th>
      <th>Cns</th>
      <th>Email</th>
      <th>Birth date</th>
      <th>Phone</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @municipes.each do |municipe| %>
      <tr>
        <td><%= municipe.name %></td>
        <td><%= municipe.cpf %></td>
        <td><%= municipe.cns %></td>
        <td><%= municipe.email %></td>
        <td><%= municipe.birth_date %></td>
        <td><%= municipe.phone %></td>
        <td><%= municipe.status %></td>
        <td><%= link_to 'Show', municipe %></td>
        <td><%= link_to 'Edit', edit_municipe_path(municipe) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= will_paginate @municipes %>

<br>

<%= link_to 'New Municipe', new_municipe_path %>
